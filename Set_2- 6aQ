void sumofanypair(int arr[],int target,int n)
{
    int sum;
    for(int i=0; i<7; i++)
    {
        for(int j=i+1; j<7; j++)
        {
            if(arr[i]+arr[j]==target)
            {
                cout << "the pair (" <<arr[i]<<" , "<<arr[j]<<") sum to "<<target<< endl;
                break;
            }
            else {
                
            }
        }
    }
}
int main()
{
    int ar[]= {1,2,3,4,5,7,11};
    int x=9;
    int n=7;
    sumofanypair(ar,x,n);
}
